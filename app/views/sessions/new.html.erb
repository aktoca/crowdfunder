<h1>Sessions#new</h1>
<h1>Sign In</h1>

<%= form_tag sessions_path do %>

  <div class="field">
    <%= label_tag :email %><br/>
    <%= text_field_tag :email %>
  </div>
  
  <div class="field">
    <%= label_tag :password %><br/>
    <%= password_field_tag :crypted_password %>
  </div>
  
  
  <div class="field">
    <%= submit_tag "Sign Up" %>
  </div>

<% end %>
